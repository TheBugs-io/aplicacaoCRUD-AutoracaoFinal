{
  "name": "aplicaco-crud-usuario",
  "version": "1.0.0",
  "description": "Aplicação para fins didáticos explicando como fazer um crud de usuários usando o node.js, express e JSON.",
  "main": "server.js",
  "type":"module",
  "scripts": {
    "start": "nodemon --env-file=.env server.js ",
    "dev": "nodemon --env-file=.env server.js",
    "generate:users": "node --env-file=.env gerar_usuarios_fake.js 100",
    "build": "npm i && node gerar_usuarios_fake.js 100"
  },
  "keywords": [
    "usuarios",
    "faker",
    "api",
    "express",
    "crud"
  ],
  "author": "Wellington Sarmento, Ingryd Duarte, Renan Araujo Polainas, David Boanerges, Tiago Viana, Arthur Heraclio",
  "license": "LGPL-3.0-or-later",
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "proper-lockfile": "^4.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
